    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.15",
    "husky": "9.1.7",
    "jest-fixed-jsdom": "0.0.11",
